<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:m="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<ValueHelpDialog title="Product" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" supportRanges="true" key="Name"
		descriptionKey="NameType">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch">
			<!--isRunningInValueHelpDialog="true">-->
				<fb:filterGroupItems>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Zzname" label="Name" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Zzname"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Zznametype" label="NameType" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Zznametype"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Zzprice" label="Price" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Zzprice"/>
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>