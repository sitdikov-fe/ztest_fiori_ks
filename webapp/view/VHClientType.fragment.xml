<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:m="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<ValueHelpDialog title="Company" ok=".onValueHelpOkPress2" cancel=".onValueHelpCancelPress2" supportRanges="true" key="Id"
		descriptionKey="NameOrg">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch2">
			<!--isRunningInValueHelpDialog="true">-->
				<fb:filterGroupItems>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Id" label="Number" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Id"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="NameOrg" label="Name" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="NameOrg"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Address" label="Address" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Address"/>
						</fb:control>
					</fb:FilterGroupItem>					
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>