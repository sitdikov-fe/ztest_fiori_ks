<core:FragmentDefinition xmlns="sap.ui.comp.valuehelpdialog" xmlns:m="sap.m" xmlns:fb="sap.ui.comp.filterbar" xmlns:core="sap.ui.core">
	<ValueHelpDialog title="Product" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" supportRanges="true" key="Ztype"
		descriptionKey="Zdesc">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch">
			<!--isRunningInValueHelpDialog="true">-->
				<fb:filterGroupItems>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Ztype" label="Type" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Ztype"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Zdesc" label="Description" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Zdesc"/>
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>